%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Comment overrides for Ada 2005 TXL grammar 
% James Cordy, May 2010
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% These overrides allow transformations that preserve comments in output.
% Care must be taken to allow for comments in all patterns that use these overrides.


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% 2_lexical_elements
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

redefine semi
    ; [eol]
end redefine

redefine blocksemi
    ; [NL] [opt comment_NL]
end redefine

redefine unitsemi
    ; [NL] [NL] [opt comment_NL]
end redefine

redefine colon
    : [opt comment_NL]
end redefine

redefine arrow
    => [opt comment_NL]
end redefine

redefine colon_equals
    [opt comment_NL] := [opt comment_NL]
end redefine

redefine eol
      [comment_NL] 
    | [NL]
end redefine

redefine blockeol
    [NL] [opt comment_NL] 
end redefine

define comment_NL
    [SP] [comment] [NL]                % only present if -comment is on
    [opt comment_NL]
end define

redefine pragma_arguments
    [opt comment_NL] ...
end redefine

redefine pragma_arguments
    ... [opt comment_NL]
end redefine

redefine pragma_argument_association
    [opt comment_NL] ... 
end redefine

redefine pragma_argument_association
    ...  [opt comment_NL]
end redefine


